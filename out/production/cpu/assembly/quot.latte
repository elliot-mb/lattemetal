-- load operands
addi t3 zero #10
addi t4 zero #2
-- a quotient division t3 / t4 into t5
muli t6 t4 #-1 --so we can subtract t4 by adding it
add t5 t3 zero --set t5 to t3
add t5 t5 t6 -- loop head
cmp t7 zero t3 -- set t7 to comparison result
jplz t7 #-2 -- loop tail
st t5 zero #0 --output result in memory

